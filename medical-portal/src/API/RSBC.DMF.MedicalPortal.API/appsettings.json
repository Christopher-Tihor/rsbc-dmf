{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft ": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "Keycloak": {
    "Config": {
      "Url": "https://common-logon-test.hlth.gov.bc.ca/auth",
      "Realm": "moh_applications",
      "ClientId": "DMFT-WEBAPP"
    },
    "InitOptions": {
      "OnLoad": "check-sso",
      "Flow": "standard",
      "ResponseMode": "fragment",
      "PkceMethod": "S256"
    }
  }
}